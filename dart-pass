//Q2 
void main(){
   
   var C = Cat();
   C.printName();
   C.printSound();
   
   
   var D = Dog();
   D.printName();
   D.printSound();
   
   var W =  Cow();
   W.printName();
   W.printSound();
     
}


abstract class Animal {
  void printName();
  void printSound();
}


class Cat extends Animal {

  String name = "CAT";
  void printName() {
    print("Name : ${name}");
  }


  String sound = "MEOW MEOW ";
  void printSound() {
    print("Sound :${sound}");
  }
}



class Dog extends Animal {

  String name = "DOG";
  void printName() {
    print("Name : ${name}");
  }


  String sound = "HOHOHO";
  void printSound() {
    print("Sound :${sound}");
  }
}



class Cow extends Animal {

  String name = "COW";
  void printName() {
    print("Name: ${name}");
  }

 
  String sound = "MOOOOOOO";
  void printSound() {
    print("Sound :${sound}");
  }
}
